{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "src": "./src",
        "assets": "/assets",
        "components": "/components",
        "pages": "/pages",
        "api": "/api",
        "context": "/context",
        "hooks": "/hooks",
        "consts": "/consts",
        "helpers": "/helpers"
      }
    }]
  ]
}
